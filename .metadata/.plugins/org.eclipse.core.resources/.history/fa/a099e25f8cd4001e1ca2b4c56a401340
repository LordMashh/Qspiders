// 28. WAJP to find fibonaci series.
public static void fibonaci(int n) {
	int n1 = 0;
	int n2 = 1;
	System.out.print(n1 + " ");
	System.out.print(n2 + " ");
	for (int i = 1; i <= n; i++) {
		int n3 = n1 + n2;
		System.out.print(n3 + " ");
		n1 = n2;
		n2 = n3;
	}
	
}
}
