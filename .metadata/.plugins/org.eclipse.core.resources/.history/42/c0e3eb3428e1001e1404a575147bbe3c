package Arrays;

public class Book implements Comparable {
	private String name;
	private int price;
	private String author;
	
	@Override
	public int compareTo(Object o) {
		Book b = (book)o;
		if (this.price > b.price) {
			return 1;
		} else (this.price < b.price){
			return -1;
		}
		return 0;
	}
}
