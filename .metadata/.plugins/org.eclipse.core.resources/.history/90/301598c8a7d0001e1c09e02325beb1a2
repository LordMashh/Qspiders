// to print largest number of 3 numbers using conditional operator.
// WAJP to check whether even number or not

package programming_classes;

import java.util.Scanner;

public class LargestOf {
public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.println("enter a number to find even or not " );
	int n = sc.nextInt();
//	if((n&1)==0)
	if(n/2*2 == n)
		System.out.println("even");
	else
		System.out.println("odd");
	
//	int a = sc.nextInt();	
//	int b = sc.nextInt();	
//	int c = sc.nextInt();	
//	
//	System.out.println( a > b ? a > c ? a + " is the largest" : c + " is the largest" : b > c ? b + " is the largest"  : c + " is the largest" );

	
}
}
